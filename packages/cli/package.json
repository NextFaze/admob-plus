{
  "name": "admob-plus",
  "description": "AdMob Plus CLI",
  "version": "0.15.3",
  "bin": {
    "admob-plus": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "clipboardy": "^2.0.0",
    "elementtree": "^0.1.7",
    "envinfo": "^7.3.1",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "lodash": "^4.17.11",
    "read-pkg-up": "^6.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/clipboardy": "^2.0.1",
    "@types/elementtree": "^0.1.0",
    "@types/fs-extra": "^7.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12",
    "chai": "^4",
    "globby": "^9",
    "mocha": "^6",
    "nyc": "^14",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "author": "Ratson",
  "license": "MIT",
  "repository": "admob-plus/admob-plus",
  "homepage": "https://admob-plus.github.io",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "admob-plus",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
